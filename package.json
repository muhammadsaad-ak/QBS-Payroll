  "scripts": {
    "serve:shell": "nx serve shell",
    "serve:admin": "nx serve admin",
    "serve:employee": "nx serve employee",
    "start:all": "nx run-many --target=serve --projects=shell,admin --parallel",
    "build:all": "nx run-many --target=build --projects=shell,admin --parallel --configuration=production",
    "build:ssr": "nx run-many --target=server --projects=shell,admin --parallel --configuration=production",
    "serve:ssr": "nx run-many --target=serve-ssr --projects=shell,admin --parallel --configuration=production",
    "serve:prod": "concurrently \"npx serve -p 4200 dist/apps/shell/browser\" \"npx serve -p 4201 dist/apps/admin/browser\"",
    "start": "nx run-many --target=serve --projects=shell,admin --parallel",
    "docker:build": "npm run build:all && docker build -t mfe-app .",
    "docker:run": "docker run -p 4200:4200 -p 4201:4201 mfe-app"
  },
